{
  "manifest_version": 3,
  "name": "Internet Download Manager Extension",
  "version": "1.0",
  "description": "Sends download requests to a local download manager.",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:8080/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  }
}